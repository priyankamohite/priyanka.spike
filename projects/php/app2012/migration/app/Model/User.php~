<?php
class User extends AppModel {
	public $name = 'User';
    public $hasOne = 'Profile';
    public $hasMany = 'Role';
	 public $validate = array(
        'id' => array(
            'rule' => 'notEmpty'
        ),
        'username' => array(
            'rule' => 'notEmpty'
        ),
        'password' => array(
            'rule' => 'notEmpty'
        )
    );
}
?>